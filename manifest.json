{
	"manifest_version": 2,
	"name": "Old Image Style",
	"version": "1.2",
	"default_locale": "en",
	
	"description": "__MSG_extensionDescription__",
	
	"icons": {
		"48": "icons/icon-48.png"
	},
	
	"content_scripts": [
		{
			"matches": [
				"http://*/*",
				"https://*/*",
				"file://*/*"
			],
			"js": ["main.js"],
			"run_at": "document_start"
		}
	]
}
